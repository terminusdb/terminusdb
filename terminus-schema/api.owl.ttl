api:WithCommitId
  a owl:Class .

api:commit_id
  a owl:DatatypeProperty ;
  rdfs:comment "The property points to a commit id string"@en ;
  rdfs:label "commit id"@en ;
  rdfs:domain api:WithCommitId ;
  rdfs:range xsd:string .

api:UnresolvableTargetAbsoluteDescriptor
  a owl:Class ;
