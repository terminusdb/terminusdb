api:DbDeleteResponse
  a owl:Class ;
  rdfs:label "DB Delete Response"@en ;
  rdfs:comment "Response document for DB Deletion."@en ;
  rdfs:subClassOf api:ResultResponse .

api:DbCreateErrorResponse
  a owl:Class ;
